<nav class="navbar navbar-toggleable-sm navbar-light px-1 py-3 my-3 mb-sm-5">
  <a class="hidden-md-down navbar-brand ml-2" href="<%- config.root %>"><%-config.title %> âˆž <%-config.subtitle %></a>
  <a class="hidden-lg-up navbar-brand ml-2" href="<%- config.root %>"><%-config.title %><br /><%-config.subtitle %></a>
  <button class="navbar-toggler navbar-toggler-right py-2" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
  <div class="collapse navbar-collapse text-center" id="navbarCollapse">
    <ul class="navbar-nav ml-auto my-auto">
      <% for (var i in config.menu){ %>
        <li class="nav-item">
          <% if(config.menu[i].search("http") === -1) {%>
            <a class="nav-link" href="<%- url_for(config.menu[i]) %>"><%= i %></a>
          <%} else { %>
            <a class="nav-link" href="<%= config.menu[i] %>" title="<%= i %>" target="_blank" rel="external"><%= i %></a>
          <% } %>
        </li>
      <% } %>
    </ul>
    <hr class="hidden-md-up" />
  </div>
</nav>
